<csp_whitelist xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Csp/etc/csp_whitelist.xsd">
    <policies>
        <policy id="script-src">
            <values>
                <value id="madar-stockavailability-script-maps-googleapis" type="host">https://maps.googleapis.com</value>
                <value id="madar-stockavailability-script-maps-gstatic" type="host">https://maps.gstatic.com</value>
            </values>
        </policy>
        <policy id="img-src">
            <values>
                <value id="madar-stockavailability-img-maps-googleapis" type="host">https://maps.googleapis.com</value>
                <value id="madar-stockavailability-img-maps-gstatic" type="host">https://maps.gstatic.com</value>
            </values>
        </policy>
        <policy id="connect-src">
            <values>
                <value id="madar-stockavailability-connect-maps-googleapis" type="host">https://maps.googleapis.com</value>
                <value id="madar-stockavailability-connect-maps-gstatic" type="host">https://maps.gstatic.com</value>
                <value id="madar-stockavailability-connect-nominatim" type="host">https://nominatim.openstreetmap.org</value>
            </values>
        </policy>
        <policy id="style-src">
            <values>
                <value id="madar-stockavailability-style-fonts-googleapis" type="host">https://fonts.googleapis.com</value>
            </values>
        </policy>
        <policy id="font-src">
            <values>
                <value id="madar-stockavailability-font-fonts-gstatic" type="host">https://fonts.gstatic.com</value>
            </values>
        </policy>
        <!--  If you embed Google Maps in an <iframe>, also add:
        <policy id="frame-src">
            <values>
                <value id="madar-stockavailability-frame-google" type="host">https://www.google.com</value>
            </values>
        </policy>
         -->
    </policies>
</csp_whitelist>